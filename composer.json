{
    "name": "prajwal89/lara-toast",
    "description": "lightweight toast notification package for laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "toast",
        "notification",
        "alert",
        "message"
    ],
    "autoload": {
        "psr-4": {
            "Prajwal89\\LaraToast\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prajwal89\\LaraToast\\LaraToastsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Prajwal Hallale",
            "email": "22263436+prajwal89@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.20"
    }
}